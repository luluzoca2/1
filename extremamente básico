#include <stdio.h>
#include <locale.h>

int main() {
    // Defina a localização para usar caracteres acentuados (pode variar dependendo do sistema)
    setlocale(LC_ALL, "Portuguese");

int a, b, valor;
printf("A: ", a);
scanf ("%d", &a);

printf("B: ", b);
scanf("%d", &b);

valor = (a+b);
printf("Resultado: %d", valor);
    
    return 0;
    
}
